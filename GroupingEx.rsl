GroupingBasics

scheme GroupingEx =
extend GroupingBasics with 
class
  value

   divide : Person-set >< Relation -~-> Grouping
   divide(ps, r) is 
   let p = formPairs(ps, r),
       pf = finalGroup(p, newPS(p, ps), r) in
   addSingles(pf, newPS(pf, ps))
   end

   pre isFriendsRelation(r)

end
